<!--WIKI INDEX-->
<div class="jumbotron">
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <h1 id="index">Wikis#index</h1>
            <p id="index">Submit a new wiki here <%= link_to "New Wiki", new_wiki_path %></p>
        </div>    
    </div>    
</div>    
<div class="container">
    <h1>All Wikis</h1>
    
    <% @wikis.each do |w| %>
        <p><%= link_to w.title, w %></p>
    <% end %>
</div>

